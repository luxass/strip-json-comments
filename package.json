{
  "name": "@luxass/strip-json-comments",
  "version": "2.0.1",
  "description": "strip comments from your json",
  "type": "module",
  "author": {
    "name": "Lucas Nørgård",
    "email": "lucasnrgaard@gmail.com",
    "url": "https://luxass.dev"
  },
  "packageManager": "pnpm@10.29.3",
  "license": "MIT",
  "homepage": "https://github.com/luxass/strip-json-comments",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luxass/strip-json-comments.git"
  },
  "bugs": {
    "url": "https://github.com/luxass/strip-json-comments/issues"
  },
  "keywords": [
    "json",
    "strip",
    "comments",
    "remove",
    "delete",
    "trim",
    "multiline",
    "parse",
    "config",
    "configuration",
    "settings",
    "util",
    "env",
    "environment",
    "jsonc"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.cts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "vitest",
    "test": "vitest --run",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@luxass/eslint-config": "7.2.0",
    "@types/node": "22.16.3",
    "eslint": "10.0.0",
    "publint": "0.3.17",
    "tsdown": "0.20.3",
    "typescript": "5.9.3",
    "vitest": "4.0.18"
  },
  "pnpm": {
    "overrides": {
      "rolldown": "1.0.0-rc.4"
    }
  }
}
